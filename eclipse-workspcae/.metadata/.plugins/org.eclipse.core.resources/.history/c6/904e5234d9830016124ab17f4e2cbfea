package com.hrsystem;

public class EmployeeId implements Comparable{
int id;
String suffix;
public EmployeeId(int i, String s) {
	id = i;
	suffix = s;
}
@Override
	public int compareTo(Object o) {
	if(this.suffix.compareTo(((EmployeeId)o).suffix)==0)
	{
		return this.id-((EmployeeId)o).id;
	}
	else return this.suffix.compareTo(((EmployeeId)o).suffix);
	}

@Override
	public String toString() {
		return id+"_"+suffix;
	}
}
